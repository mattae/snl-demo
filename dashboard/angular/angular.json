{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "ngx-dashboard": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "src/main",
            "sourceRoot": "src/main",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "ngx-build-plus:browser",
                    "options": {
                        "outputPath": "target/classes/static/js/ngx-dashboard",
                        "index": "src/main/webapp/index.html",
                        "main": "src/main/webapp/main.ts",
                        "polyfills": [
                        ],
                        "tsConfig": "tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            {
                                "glob": "favicon.ico",
                                "input": "src/main/webapp",
                                "output": "/"
                            }
                        ],
                        "styles": [
                            "src/main/webapp/styles.scss"
                        ],
                        "extraWebpackConfig": "webpack/webpack.prod.config.js"
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/main/webapp/environments/environment.ts",
                                    "with": "src/main/webapp/environments/environment.prod.ts"
                                }
                            ],
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kB",
                                    "maximumError": "1MB"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "4kB",
                                    "maximumError": "8kB"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "optimization": false,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "assets": [
                                {
                                    "glob": "**/*",
                                    "input": "src/main/webapp/assets/",
                                    "output": "/assets/"
                                },
                                {
                                    "glob": "favicon.ico",
                                    "input": "src/main/webapp",
                                    "output": "/"
                                },
                                {
                                    "glob": "mockServiceWorker.js",
                                    "input": "src/main/webapp/assets",
                                    "output": "/"
                                },
                                {
                                    "glob": "**/*",
                                    "input": "src/main/resources/installers/ngx-dashboard",
                                    "output": "/static/"
                                }
                            ],
                            "styles": [
                                "src/main/webapp/styles.scss",
                                {
                                    "input": "src/main/webapp/styles-dev.scss",
                                    "bundleName": "dev"
                                }
                            ]
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "ngx-build-plus:dev-server",
                    "configurations": {
                        "production": {
                            "buildTarget": "ngx-dashboard:build:production"
                        },
                        "development": {
                            "buildTarget": "ngx-dashboard:build:development"
                        }
                    },
                    "options": {
                        "buildTarget": "ngx-dashboard:build:development",
                        "port": 22856,
                        "extraWebpackConfig": "webpack/webpack.dev.js"
                    },
                    "defaultConfiguration": "development"
                }
            }
        }
    },
    "cli": {
        "packageManager": "yarn",
        "analytics": "a7f3efbe-a5fe-4101-b9e8-826d111832da"
    }
}


